#include <stdio.h>

int main()
{
    int data[100], n, temp, i, j;

    printf("Enter number of terms (should be less than 100): ");
    scanf("%d", &n);

    if (n <= 0 || n > 100)
    {
        printf("Invalid number of terms.\n");
        return 1;
    }

    printf("Enter elements:\n");
    for (i = 0; i < n; i++)
    {
        scanf("%d", &data[i]);
    }

    // Insertion Sort Logic
    for (i = 1; i < n; i++)
    {
        temp = data[i];
        j = i - 1;

        while (j >= 0 && temp < data[j])
        {
            data[j + 1] = data[j];
            j--;
        }
        data[j + 1] = temp;
    }

    printf("In ascending order:\n");
    for (i = 0; i < n; i++)
        printf("%d\t", data[i]);

    return 0;
}
