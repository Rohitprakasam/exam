#include <stdio.h>
#include <conio.h>
#define MAX 5

int queue[MAX];
int front = -1, rear = -1;

void enqueue(int x)
{
    if (rear == MAX - 1)
        printf("\nQueue Overflow");
    else
    {
        if (front == -1)
            front = 0;
        queue[++rear] = x;
        printf("\nInserted: %d", x);
    }
}

void dequeue()
{
    if (front == -1 || front > rear)
        printf("\nQueue Underflow");
    else
        printf("\nDeleted: %d", queue[front++]);
}

void display()
{
    int i;
    if (front == -1 || front > rear)
        printf("\nQueue is Empty");
    else
    {
        printf("\nQueue Elements: ");
        for (i = front; i <= rear; i++)
            printf("%d ", queue[i]);
    }
}

void main()
{
    clrscr();

    enqueue(10);
    enqueue(20);
    enqueue(30);

    display();

    dequeue();
    display();

    getch();
}
